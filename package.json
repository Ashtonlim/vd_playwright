{
  "name": "vaultdragone2e",
  "version": "1.0.0",
  "description": "end to end testing for vault dragon",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "npm run test:integration:sequential && npm run test:integration:parallel",
    "test:integration:parallel": "jest --config=__tests__/integration/jest.config.js",
    "test:integration:sequential": "jest --config=__tests__/integration/jest.config.js --runInBand",
    "test:unit": "jest --config=__tests__/unit/jest.config.js --passWithNoTests",
    "t": "jest ./tests/doing/template1",
    "reset": "jest ./tests/resets",
    "main": "jest ./tests/main",
    "rec": "npx playwright codegen --viewport-size=1920,1080 --load-storage=creds.json -o ./codeOutput/out.js https://hub-staging.vaultdragon.com",
    "emrec": "npx playwright codegen --viewport-size=1920,1080 -o ./codeOutput/out.js https://mail.zoho.com/zm/#mail/folder/inbox/p/1622723244776100001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ashtonlim/vd_playwright.git"
  },
  "author": "ash",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ashtonlim/vd_playwright/issues"
  },
  "homepage": "https://github.com/Ashtonlim/vd_playwright#readme",
  "jest": {
    "preset": "jest-playwright-preset"
  },
  "devDependencies": {
    "dir-compare": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "jest-html-reporters": "^2.1.6",
    "jest-playwright-preset": "^1.5.2",
    "klaw-sync": "^6.0.0",
    "micromatch": "^4.0.4",
    "playwright": "^1.11.0",
    "prettier": "^2.3.0"
  },
  "dependencies": {
    "aws-sdk": "^2.927.0",
    "dayjs": "^1.10.5",
    "dotenv": "^10.0.0"
  }
}
